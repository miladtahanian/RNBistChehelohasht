{
  "expo": {
    "name": "2048",
    "slug": "bistchehelohasht",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/2048logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/2048logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#57407c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/2048logo.png",
        "backgroundColor": "#57407c"
      },
      "package": "ir.tahanian.bistchehelohast"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/2048logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f21e294c-2db0-4ddb-b968-e81224f2ca12"
      }
    },
    "owner": "miladtahanian"
  }
}
